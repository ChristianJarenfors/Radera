@model IEnumerable<Radera.Models.Auction>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Picture)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PriceBuyout)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <img style="width:100px; height:100px" src="@item.Picture" />
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PriceBuyout)
        </td>
        
    </tr>
}

</table>
