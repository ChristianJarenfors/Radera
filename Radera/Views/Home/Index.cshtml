
@{
    ViewBag.Title = "Index";
}


<div class="row">
    <div class="col-lg-2 col-lg-offset-5">
        <h2>Welcome!</h2>

        <form method="get">
            <div class="form-group">
                <label for="username">Username</label><input class="form-control" type="text" name="username" placeholder="Username" />
            </div>

            <div class="form-group">
                <label for="password">Password</label><input class="form-control" type="password" name="password" placeholder="Password" />
            </div>

            <div class="form-group">
                <button class="btn btn-success" type="submit">Logga in</button>
            </div>
        </form>
    </div>
</div>
